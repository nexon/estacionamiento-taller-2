@model Taller.Estacionamiento.Models.Personal

<div class="modal fade" id="modalEliminar" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <!-- form start-->
        @using (Html.BeginForm("personalEliminar", "Estacionamiento", FormMethod.Post, new { @class = "form-horizontal" }))
        {
            @Html.AntiForgeryToken()
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">Eliminar Personal</h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-xs-12">
                            <form class="form-horizontal" role="form">
                                
                                <div class="form-group">
                                    @Html.LabelFor(model => model.Nombre, htmlAttributes: new { @class = "col-sm-3 control-label no-padding-right" })
                                    <div class="col-sm-9">
                                        @Html.TextBoxFor(model => model.Nombre, new { @class = "col-xs-12", @id = "personal-eliminar-nombre", disabled = "disabled" })
                                        @Html.HiddenFor(model => model.Nombre, new { @id = "personal-eliminar-nombre-hidden" })
                                        @Html.ValidationMessageFor(model => model.Nombre, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                                <div class="form-group">
                                    @Html.LabelFor(model => model.Rut, htmlAttributes: new { @class = "col-sm-3 control-label no-padding-right" })
                                    <div class="col-sm-9">
                                        @Html.TextBoxFor(model => model.Rut, new { @class = "col-xs-12", @id = "personal-eliminar-rut", disabled = "disabled" })
                                        @Html.HiddenFor(model => model.Rut, new {@id = "personal-eliminar-rut-hidden"})
                                        @Html.ValidationMessageFor(model => model.Rut, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                                <div class="form-group">
                                    @Html.LabelFor(model => model.Email, htmlAttributes: new { @class = "col-sm-3 control-label no-padding-right" })
                                    <div class="col-sm-9">
                                        @Html.TextBoxFor(model => model.Email, new { @class = "col-xs-12", @id = "personal-eliminar-email", disabled = "disabled" })
                                        @Html.HiddenFor(model => model.Email, new { @id = "personal-eliminar-email-hidden" })
                                        @Html.ValidationMessageFor(model => model.Email, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                                <div class="form-group">
                                    @Html.LabelFor(model => model.Telefono, htmlAttributes: new { @class = "col-sm-3 control-label no-padding-right" })
                                    <div class="col-sm-9">
                                        @Html.TextBoxFor(model => model.Telefono, new { @class = "col-xs-12", @id = "personal-eliminar-telefono", disabled = "disabled" })
                                        @Html.HiddenFor(model => model.Telefono, new { @id = "personal-eliminar-telefono-hidden" })
                                        @Html.ValidationMessageFor(model => model.Telefono, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                            </form>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-sm" data-dismiss="modal">
                        <i class="ace-icon fa fa-arrow-left bigger-110"></i>
                        Cancelar
                    </button>
                    <button type="submit" value="Submit" class="btn btn-sm btn-success">
                        <i class="ace-icon fa fa-check bigger-110"></i>
                        Aceptar
                    </button>
                </div>
            </div>
        }
        <!-- form end-->
    </div>
</div>
