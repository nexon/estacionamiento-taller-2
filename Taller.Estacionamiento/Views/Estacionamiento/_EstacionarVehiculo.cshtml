<div id="EstacionarVehiculoModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Estacionar nuevo vehiculo</h4>
            </div>
            <div class="modal-body">
                <p>Ingrese la patente del vehiculo a estacionar.</p>
                @using (Html.BeginForm("EstacionarVehiculo", "Estacionamiento", FormMethod.Post))
                {
                    <input type="hidden" id="ID" class="form-control" value="" disabled />
                    <div class="form-group">
                        <label for="Nombre">Estacionamiento</label>
                        <input type="text" id="Nombre" class="form-control" value="" disabled />
                    </div>
                    <div class="form-group">
                        <label for="Nombre">Espacio</label>
                        <input type="text" id="codigo" class="form-control" value="" disabled />
                    </div>
                    <div class="form-group">
                        <label for="Nombre">Patente</label>
                        <input type="text" class="form-control" placeholder="La patente del vehiculo" />
                    </div>
                    <input type="submit" class="btn btn-primary" value="Estacionar" />
                }
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>

