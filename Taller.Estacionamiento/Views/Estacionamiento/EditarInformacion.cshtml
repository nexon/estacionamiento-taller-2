@{
    ViewBag.Title = "EditarInformacion";

        }

<div class="page-header">
    <h1>
        Estacionamiento
        <small>
            <i class="ace-icon fa fa-angle-double-right"></i>
            Información
        </small>
        <small>
            <i class="ace-icon fa fa-angle-double-right"></i>
            Editar
        </small>
    </h1>
</div><!-- /.page-header -->

<div class="row">
    <div class="col-xs-12">
        <form class="form-horizontal" role="form">
            <!-- #section:elements.form -->
            <div class="form-group">
                <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> Nombre Estacionamiento </label>
                <div class="col-sm-9">
                    <input type="text" id="form-field-1" class="col-xs-10 col-sm-5" />
                </div>
            </div>

            <div class="form-group">
                <label class="col-sm-3 control-label no-padding-right" for="form-field-mask-2"> Telefono </label>

                <div class="col-xs-10 col-sm-5">
                    <div class="input-group">
                        <div class="input-group-addon">
                            <span>
                                <i class="ace-icon fa fa-phone"></i>
                            </span>
                        </div>
                        <input type="text" id="form-field-mask-2" class="col-xs-10 col-sm-9" />
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label class="col-sm-3 control-label no-padding-right" for="form-field-mask-3"> Correo </label>

                <div class="col-xs-10 col-sm-5">
                    <div class="input-group">
                        <div class="input-group-addon">
                            <span>
                                <i class="ace-icon fa fa-envelope"></i>
                            </span>
                        </div>
                        <input type="text" id="form-field-mask-3" class="col-xs-10 col-sm-9" />
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label class="col-sm-3 control-label no-padding-right" for="form-field-mask-4"> Horario apertura </label>
                <div class="col-xs-10 col-sm-5">
                    <div class="input-group">
                        <div class="input-group-addon">
                            <span>
                                <i class="ace-icon fa fa-clock-o"></i>
                            </span>
                        </div>
                        <input type="text" id="form-field-mask-4" class="col-xs-5 col-sm-5" />
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label class="col-sm-3 control-label no-padding-right" for="form-field-mask-5"> horario cierre </label>
                <div class="col-xs-10 col-sm-5">
                    <div class="input-group">
                        <div class="input-group-addon">
                            <span>
                                <i class="ace-icon fa fa-clock-o"></i>
                            </span>
                        </div>
                        <input type="text" id="form-field-mask-5" class="col-xs-5 col-sm-5" />
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label class="col-sm-3 control-label no-padding-right" for="form-field-mask-5"> 24 Hrs </label>
                <div class="col-xs-3">
                    <label>
                        <input id="24hrs-check" name="switch-field-1" class="ace ace-switch" type="checkbox">
                        <span class="lbl" data-lbl="SI&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;NO"></span>
                    </label>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-3 control-label no-padding-right" for="form-field-6"> Ubicación </label>
                <div class="col-lg-5">
                    <div id="map" class="map"></div>
                </div>
            </div>

            <div class="clearfix form-actions">
                <div class="col-md-offset-3 col-md-9">
                    <button class="btn btn-info" type="button">
                        <i class="ace-icon fa fa-check bigger-110"></i>
                        Guardar
                    </button>

                    &nbsp; &nbsp; &nbsp;
                    <button class="btn" type="reset">
                        <i class="ace-icon fa fa-remove bigger-110"></i>
                        Cancelar
                    </button>
                </div>
            </div>
        </form>
     </div>
</div>
@section ViewsScripts {
    <script type="text/javascript" src="../Scripts/ol.js"></script>
    <script type="text/javascript" src="../Scripts/informacion/editar.js"></script>
}
